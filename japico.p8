 print("to leave game",72,8,textcol)
 if(not gameover) then
  inc = 0
  for char in all(c) do
   sspr(char.sprx,char.spry,8,8,((inc%10)*26)+16,(flr(inc/10)*9)+32)
   inc += 1
  end
 end

