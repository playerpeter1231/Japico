pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
function _init()
 bgcolor = 0
 --rnum = flr(rnd(maxrnd)+1) --random number variable
 kstr = "" --for handling keyboard strings

 poke(0x5f2d,1)
 menu_init()
 type_init()
 init_keys()
 match_init()

 --chars_init()
end

function menu_init()
 gstate = 0 -- 0 for main menu
            -- 1 for type game
            -- 2 for match game
            -- 3 for write game
            -- 4 for late game
 mopt=1     --menuoption
 sopt=1     --sideoption
 mxoff = -7  --menu x offset
 myoff = -8 --menu y offset
 timer = -1
 poptime = 70
end

function type_init()
 textcol = 5
 setup1 = true
 setup2 = false
 gameover=false
 debug = false
 useit = 1 --iterator for used table
 maxw = 0 --maximum wrong answers
 maxm = 0 --maximum missed letters
 numw = 0 --number of wrong answers
 numm = 0 --number of missed letters
 currscore = 0
 hiscore = 0
 wrong = {}
 miss = {}
 used = {}  --holds chars for unique rndization
end

function chars_init()
 maxrnd = 72
 chararray = {"n","wa","wi","we","wo","ra","ri","ru",
              "re","ro","ya","yu","yo","ma","mi","mu",
              "me","mo","ha","hi","hu","he","ho","na",
              "ni","nu","ne","no","ta","ti","tu","te",
              "to","sa","si","su","se","so","ka","ki",
              "ku","ke","ko","a","i","u","e","o",
              "ba","bi","bu","be","bo","da","dji","dzu",
              "de","do","za","ji","zu","ze","zo","ga",
              "gi","gu","ge","go","pa","pi","pu","pe","po"}
 c = {} --character table
 if(kata or bothalph) then
  for i=1,73 do
   char = {}
   char.sprx = ((i+2)%16) * 8 
   char.spry = flr((i+2)/16) * 8
   char.pho = chararray[i]
   char.acc1 = false
   char.acc2 = false
   if(i>48 and i<69) char.acc1 = true
   if(i>48 and i<54) then
    char.sprx = ((i-28)%16)*8
    char.spry = flr((i-28)/16)*8
   elseif(i>53 and i<69) then
    char.sprx = ((i-23)%16)*8
    char.spry = flr((i-23)/16)*8
   elseif(i>68) then
    char.acc2 = true
    char.sprx = ((i-48)%16)*8
    char.spry = flr((i-48)/16)*8
   end
   add(c,char)
  end
 end

 if(bothalph or not kata) then
  for i=0,72 do
   char = {}
   char.sprx = (i%16) * 8 
   char.spry = (flr(i/16) * 8)+64
   char.pho = chararray[(i+1)]
   char.acc1 = false
   char.acc2 = false
   if(i>47 and i<68) then
    char.acc1 = true
    char.sprx = ((i-30)%16)*8
    char.spry = (flr((i-30)/16)*8)+64
   elseif(i>67) then
    char.acc2 = true
    char.sprx = ((i-50)%16)*8
    char.spry = flr((i-50)/16)*8+64
   end
   add(c,char)
  end
 end

 if(bothalph) maxrnd = 145

 rnum = flr(rnd(maxrnd)+1)
end

function match_init()
--random indexing for randomizing keyboard
 rndind73 = {}
 rndind146 = {}
 for i=1, 72 do
  add(rndind73,i)
  add(rndind146,i)  
 end
 for i=73,146 do
  add(rndind146,i)
 end

 for i=1, 71 do
  rndswp = rnd(72-i)+i
  value = rndind73[i]
  rndind73[i] = rndind73[rndswp]
  rndind73[rndswp] = value
 end

 for i=1, 145 do
  rndswp = rnd(146-i)+i
  value = rndind146[i]
  rndind146[i] = rndind146[rndswp]
  rndind146[rndswp] = value
 end

end

function _update()
 upd_keys()
 if(gstate == 0) then
  menu_update()
 elseif(gstate == 1) then
  k_update()
  type_update()
 elseif(gstate == 2) then
  match_update()
 end
end

function menu_update()
 bgcolor = 6

 if(is_released(2)) then
  mopt-=1
  if(mopt<1) then
   mopt = 4
  end
 elseif(is_released(3)) then
  mopt+=1
  if(mopt>4) then
   mopt = 1
   end
 elseif(is_released(0)) then
  sopt-=1
  if(sopt<1)then
   sopt = 3
  end
 elseif(is_released(1)) then
  sopt+=1
  if(sopt>3)then
   sopt = 1
  end
 elseif(is_released(4) and timer == -1) then
  sfx(0)
  timer = 90
 end

 if(sopt == 1) then
  kata = true
  bothalph = false
 elseif(sopt == 2) then
  kata = false
  bothalph = false
 else
  bothalph = true
 end

 if(timer > 0) then
  timer -= 1
 elseif(timer == 0) then
  timer = -1 --stopped position
  input = stat(31)
  input = ""
  chars_init()
  gstate = mopt
 end
end

function k_update()
 maxsize = #kstr
 input = stat(31)
 if(input== "\b") then
  sfx(5)
  kstr = sub(kstr,0,#kstr-1)
 elseif(input != "\r" and #kstr < 5) then
  kstr = kstr..input
  kout = kstr
 end
 if(maxsize<#kstr) sfx(1)
end

function type_reset()
 currscore = 0
 for d in pairs(miss) do
  miss[d] = nil
 end
 for d in pairs(wrong) do
  wrong[d] = nil
 end
 for d in pairs(used) do
  used[d] = nil
 end
 for d in pairs(c) do
  c[d] = nil
 end
 chars_init()
 gameover = false
 debug = false
 numw = 0
 numm = 0
 setup1 = true
 textcol = 5
end

function unique_rand()
 add(used,c[rnum])
 useit += 1
 unique = true
 while(unique) do
  rnum=flr(rnd(maxrnd)+1)
  recent = 1
  unique = false
  for check in all(used) do
   if(c[rnum] == check and recent > useit -5) unique = true
   recent += 1
  end
 end
end

function type_update()
 bgcolor = 0

 if(debug) then
  if(is_released(2)) rnum += 1
  if(is_released(3)) rnum -= 1
 end
 if(input == "\r") then
  if(kout == "retry") then
   type_reset()
   kout = ""
   kstr = ""
  elseif(kout == "done") then
    type_reset()
    gstate = 0
  end
 end
 if(not gameover) then
  if(input == "\r") then
   if(setup1 and tonum(kout)!=nil) then
    if(tonum(kout)>-1 and tonum(kout)<11) then
     maxw = tonum(kout)
     setup1=false
     setup2=true
    end
   elseif(setup2 and tonum(kout)!=nil) then
    if(tonum(kout)>-1 and tonum(kout)<16) then
     maxm = tonum(kout)
     sfx(4)
     timer = 60
    end
   elseif(c[rnum].pho == kout) then
    --bgcolor += 1
    sfx(2)
    unique_rand()
    currscore += 1
    numw = 0
    for d in pairs(wrong) do
     wrong[d] = nil
    end
    if(currscore > hiscore) hiscore = currscore
   elseif(kout == "") then
   elseif(kout == "debug") then debug = true
   elseif(not setup1 and not setup2) then
    sfx(3)
    if(numw >= maxw - 1) then
     for d in pairs(wrong) do
      wrong[d] = nil
     end
     add(miss,c[rnum])
     numw = 0
     numm += 1
     if(numm >= maxm) then
      gameover = true
     end
     unique_rand()
    else
     add(wrong,kout)
     numw += 1
    end
   end
   kout = ""
   kstr = ""
  end
 end
 if(timer > 0) then
  timer -= 1
  if(timer == 10) then
   textcol +=1
  elseif(timer == 0) then
   textcol +=1
   setup2 = false
   timer = -1
  end
 end
end

function match_update()
  bgcolor = 0
end

function _draw()
 cls(bgcolor)
 if(gstate == 0) then
  menu_draw()
 elseif(gstate == 1) then
  type_draw()
 elseif(gstate == 2) then
  match_draw()
 end
end

